{
  "name": "sahar",
  "version": "1.0.0",
  "repository": "github:ernilambar/sahar",
  "scripts": {
    "build": "gulp build",
    "build:pot": "yarn run textdomain && yarn run language",
    "deploy": "gulp deploy",
    "language": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy",
    "prod": "gulp build && yarn run rtl && yarn run build:pot && gulp images",
    "rtl": "rtlcss style.css style-rtl.css",
    "start": "gulp",
    "textdomain": "wpi18n addtextdomain"
  },
  "author": "Nilambar Sharma",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "autoprefixer": "^9.8.4",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.1",
    "concurrently": "^5.2.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-line-ending-corrector": "^1.0.3",
    "gulp-load-plugins": "^1.6.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-rtlcss": "^1.4.1",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.0.9",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.0.1",
    "imagemin-cli": "^6.0.0",
    "node-sass": "^4.14.1",
    "node-wp-i18n": "^1.2.3",
    "onchange": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "rtlcss": "^2.5.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  }
}
